  "sqlTemplates": {
    "select": "select t.repttempid id,t.categoryid,t.repttemp,t.isabnormal,t.deleted,t.sequence,CASE WHEN t.cstudyresult IS NOT NULL THEN t.cstudyresult ELSE t.studyresult END AS cstudyresult, CASE WHEN t.cdiagresult IS NOT NULL THEN t.cdiagresult ELSE t.diagresult END AS cdiagresult,t.studyresult,t.diagresult from t_repttemp_share t,t_repttempcategory_share r where t.categoryid=r.categoryid and r.categoryid='{SUPER}{ID}' and r.deleted='0' and t.repttemp like '%{REPTTEMP}%' order by t.sequence",
    "insert": "insert into t_repttemp_share (repttempid, repttemp, cstudyresult, cdiagresult, studyresult, diagresult, isabnormal, deleted, categoryid, sequence) values ('{ID}', '{REPTTEMP}', '{CSTUDYRESULT}', '{CDIAGRESULT}', '{STUDYRESULT}', '{DIAGRESULT}', TO_NUMBER('{ISABNORMAL}'), '0', '{SUPER}{ID}', '{SEQUENCE}')",
    "update": "update t_repttemp_share set repttemp = '{REPTTEMP}', cstudyresult = '{CSTUDYRESULT}', cdiagresult = '{CDIAGRESULT}', studyresult = '{STUDYRESULT}', diagresult = '{DIAGRESULT}', isabnormal = TO_NUMBER('{ISABNORMAL}') where repttempid = '{ID}'",
    "setState": "update t_repttemp_share set deleted = '{DELETED}' where repttempid = '{ID}'",
	"delete": "delete from t_repttemp_share where repttempid = '{ID}'",
	"setSequence":"update t_repttemp_share set sequence = '{SEQUENCE}' where repttempid = '{ID}'",
    "exportData": "select e.*,e.repttempid id from t_repttemp_share e where 1=1 order by sequence"
  },
