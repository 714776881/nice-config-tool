  "sqlTemplates": {
    "select": "select t.examitemid id,t.*,b.modality,e.equipmentid,his.his_examitemid from t_examitem t inner join t_bodypart b on t.bodypartid=b.bodypartid left join t_examitemlink e on e.examitemid=t.examitemid left join t_his_examitemlink his on his.ris_examitemid=t.examitemid where t.departmentid = '{GLOBAL}{DEPARTMENTID}' and t.bodypartid = '{SUPER}{ID}' order by t.sequence",
    "insert": "insert into t_examitem (examitemid, examitem, examitemcode, srm, examitemeng, price, register_factor, tech_factor, report_factor, factor1, reportverify_factor, booking, booking_am, booking_limit_am, booking_pm, booking_limit_pm, deleted, sequence, bodypartid, departmentid,hospitalcode,regionid) values ('{ID}', '{EXAMITEM}', '{EXAMITEMCODE}', '{SRM}', '{EXAMITEMENG}', '{PRICE}', '{REGISTER_FACTOR}', '{TECH_FACTOR}', '{REPORT_FACTOR}', '{FACTOR1}', '{REPORTVERIFY_FACTOR}', '{BOOKING}', '{BOOKING_AM}', '{BOOKING_LIMIT_AM}', '{BOOKING_PM}', '{BOOKING_LIMIT_PM}', '0', '{SEQUENCE}', '{SUPER}{ID}', '{GLOBAL}{DEPARTMENTID}', '{GLOBAL}{HOSPITALID}', '{GLOBAL}{REGIONID}')[;]insert into t_his_examitemlink(link_id,ris_examitemid,his_examitemid,hospitalid) values ('{ID}', '{ID}', '{HIS_EXAMITEMID}', '{GLOBAL}{HOSPITALID}')[;]insert into t_examitemlink(linkseq,examitemid,equipmentid,dflag) values ('{ID}', '{ID}', '{EQUIPMENTID}', '1')",
    "update": "update t_examitem set examitem = '{EXAMITEM}', examitemcode = '{EXAMITEMCODE}', srm = '{SRM}', examitemeng = '{EXAMITEMENG}', price = '{PRICE}', register_factor = '{REGISTER_FACTOR}', tech_factor = '{TECH_FACTOR}', report_factor = '{REPORT_FACTOR}', factor1 = '{FACTOR1}', reportverify_factor = '{REPORTVERIFY_FACTOR}', booking = '{BOOKING}', booking_am = '{BOOKING_AM}', booking_limit_am = '{BOOKING_LIMIT_AM}', booking_pm = '{BOOKING_PM}', booking_limit_pm = '{BOOKING_LIMIT_PM}' where examitemid = '{ID}'[;]update t_his_examitemlink set ris_examitemid = '{ID}', his_examitemid = '{HIS_EXAMITEMID}' where  link_id = '{ID}' or ris_examitemid = '{ID}' [;] update t_examitemlink set equipmentid = '{EQUIPMENTID}' where examitemid = '{ID}' or linkseq = '{ID}'",
    "setState": "update t_examitem set deleted = '{DELETED}' where examitemid = '{ID}'",
	"delete": "delete from t_examitem where examitemid = '{ID}'",
	"setSequence":"update t_examitem set sequence = '{SEQUENCE}' where examitemid = '{ID}'",
    "exportData": "select t.examitemid id,t.*,b.modality from t_examitem t,t_bodypart b where t.bodypartid=b.bodypartid and t.departmentid = '{GLOBAL}{DEPARTMENTID}' and t.bodypartid = '{SUPER}{ID}' order by t.sequence"
  },
