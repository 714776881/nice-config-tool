<config>
	<!--工具栏配置,RAD对应放射科，US对应超声科，ES对应内镜科-->
	<item name="ToolbarConfiguration">
		<!--Worklist配置对应科室下的Worklist界面的工具栏-->
		<!--Report配置对应科室下的报告界面的工具栏-->
		<item name="US">
			<item name="Worklist" value="#Array">
				<item value="#Array">
					<item>
						<item name="Name" value="WriteReport"/>
					</item>
				</item>
				<item value="#Array">
					<item>
						<item name="Name" value="ModifyReport"/>
					</item>
				</item>
				<item value="#Array">
					<item>
						<item name="Name" value="ShowReport"/>
					</item>
				</item>
				<item value="#Array">
					<item>
						<item name="Name" value="ViewOrderComment"/>
					</item>
				</item>
			</item>
		</item>
	</item>
</config>