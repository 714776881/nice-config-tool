    <!--设置导出图像的配置-->
    <item name="ExportImageConfig">
      <item name="UseOriginalSize" value="false" />
      <item name="LeastSize" value="512" />
      <!--导出图像时是否进行缩放，默认为1，不进行缩放，本选项对web版无效，只对桌面版导出当前图像有效-->
      <item name="ScaleRatio" value="1" />
      <!-- 目前支持的导出格式有bmp、png、jpg、gif及tiff, 其中bmp为默认格式， avi未来会支持-->
      <item name="ExportFileFormat" value="jpg" />
      <item name="SaveToRemoteComputer" value="true" />
	  <item name="HidePatientName" value="true" />
	  <item name="HideCornerInfo" value="true" />
	  <!-- 导出的dcm图像脱敏时需要清空内容的tag-->
	  <item name="HideCornerInfoForDCMTags" value="patientname;pid;accno;sex;birthday;age;examdate;examtime;seriesdate;seriestime;InstitutionName;acqdate;acqtime;" />
	  <!-- 导出文件夹第一级目录名称组成；patientname;pid;examitem(检查项目);-->
	  <!-- examtime(检查日期);modality;accno;timeStamp;使用；分隔开-->
	  <!-- 不配置此项目，或者配置为空，表示不创建这一级目录;如果两级目录都不做配置，则默认pid一层目录-->
