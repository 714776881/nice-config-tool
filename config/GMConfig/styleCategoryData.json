    "select": "select s.styleid id,s.* from t_style s where s.stylecategoryid = '{SUPER}{ID}'",
    "insert": "insert into t_style(styleid,stylecategoryid,style,styletype1,styletype2,styletype3,styletype4,styletype5,printername,filmnum,content,active,deleted,sequence) values('{ID}','{SUPER}{ID}','{STYLE}','{STYLETYPE1}','{STYLETYPE2}','{STYLETYPE3}','{STYLETYPE4}','{STYLETYPE5}','{PRINTERNAME}','{FILMNUM}','{CONTENT}','{ACTIVE}','{DELETED}','{SEQUENCE}')",
    "update": "UPDATE t_style SET stylecategoryid = '{SUPER}{ID}', style = '{STYLE}', styletype1 = '{STYLETYPE1}', styletype2 = '{STYLETYPE2}', styletype3 = '{STYLETYPE3}', styletype4 = '{STYLETYPE4}', styletype5 = '{STYLETYPE5}', printername = '{PRINTERNAME}', filmnum = '{FILMNUM}', content = '{CONTENT}', active = '{ACTIVE}', deleted = '{DELETED}', sequence = '{SEQUENCE}' WHERE styleid = '{ID}';",
    "setState": "update t_style set deleted = '{DELETED}' where styleid = '{ID}'",
	"delete": "delete from t_style where styleid = '{ID}'",
	"setSequence":"update t_style set sequence = '{SEQUENCE}' where styleid = '{ID}'",
    "exportData": "select e.*,e.styleid id from t_style e where 1=1 order by sequence"
